<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cat_cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    card_view:cardBackgroundColor="@color/pink"
    card_view:cardCornerRadius="12dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp">


        <TextView
            android:id="@+id/card_ev_id"
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="5dp"
            android:text="EWW-00000"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/textView14"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/card_ev_name"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="5dp"
            android:text="smtsmtmsmt"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/textView16"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/cardE_cat_id"
            android:layout_width="72dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="8dp"
            android:text="CWW-0000"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/textView15"
            card_view:layout_constraintTop_toBottomOf="@+id/card_ev_id" />

        <TextView
            android:id="@+id/card_ticket"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="8dp"
            android:text="100000"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/textView17"
            card_view:layout_constraintTop_toBottomOf="@+id/card_ev_name" />

        <TextView
            android:id="@+id/cardE_is_active"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="18dp"
            android:text="Inactive"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:text="ID:"
            android:textStyle="bold"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="8dp"
            android:text="Category ID:"
            android:textStyle="bold"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/card_ev_id" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:text="Name:"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/card_ev_id"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Tickets:"
            android:textStyle="bold"
            card_view:layout_constraintStart_toEndOf="@+id/cardE_cat_id"
            card_view:layout_constraintTop_toBottomOf="@+id/card_ev_name" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>